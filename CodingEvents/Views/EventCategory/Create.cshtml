@using CodingEvents.ViewModels;

@model AddEventCategoryViewModel;

<h1>Add New Category</h1>
<br />
<form asp-controller="EventCategory" asp-action="ProcessCreateEventCategoryForm" method="post">
    <div class="form-group">
        <label asp-for="Name"></label>
        <input asp-for="Name" />
        <span asp-validation-for="Name"></span>
    </div>

    <input type="submit" value="Create Category Name" />
</form>
